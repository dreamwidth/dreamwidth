[% USE Dumper %]
<!DOCTYPE html>
<html>
<head>
<title>[% IF view == "recent" %]Recent[% ELSE %]Tag[% END %] Page | [% u.username %]</title>
    [% INCLUDE "mobile/head.tt" %]
</head>
<body>
<div class="[% view %]-page entries-page" data-role="page">
	<div data-role="header">
	<h1>[% IF view == "recent" %]Recent[% ELSE %]Tag[% END %] Page | [% u.username %]</h1>
	</div>
	[% INCLUDE "mobile/entry-list.tt"
	    entries = entries,
	    backcount = backcount,
	    forwardcount = forwardcount,
	    itemsperpage = itemsperpage
	    %]
</body>
</html>
