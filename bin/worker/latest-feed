use strict;
BEGIN {
    require "$ENV{LJHOME}/cgi-bin/ljlib.pl";
}

use LJ::Worker::TheSchwartz;
use DW::Worker::LatestFeed;

schwartz_decl( "DW::Worker::LatestFeed" );
schwartz_work();
