function Page::print_theme_stylesheet() {
    """
    blockquote {
        color: inherit;
        border: none;
        font-size: inherit;
        font-style: normal;
        margin-top: .9em;
        margin-bottom: .9em;
        padding: 1em;
        }

    a:hover {
        text-decoration: underline;
        }

    #primary, #tertiary,
    #comment, #footer {
        width: 65%;
        text-align: left;
        }

    .entry, .entry .inner,
    .comment, .comment .inner {
        line-height: 1.05em;
        }

    .tag-text,
    .tag ul li,
    .metadata ul li {
        font-size: .9em;
        }

    .tag ul {
        padding: 0;
        }
    """;
}


