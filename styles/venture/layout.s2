    }

.talkform #subject { margin-bottom: .5em; }

.talkform #prop_picture_keyword, .talkform #randomicon, .talkform #quotebuttonspan {
    display: inline-block;
    margin-top: 0.4em;
    }

.talkform tr:last-child td + td {
